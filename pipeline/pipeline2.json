{
	"name": "pipeline2",
	"properties": {
		"activities": [
			{
				"name": "IterateSQLTables",
				"type": "ForEach",
				"typeProperties": {
					"items": {
						"value": "@activity('Copy Data2').output",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "Copy Data2",
							"type": "Copy",
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"typeProperties": {
								"source": {
									"type": "AzureSqlSource",
									"sqlReaderQuery": "SELECT SOURCE_TBL FROM LOAD_CFG"
								},
								"sink": {
									"type": "DelimitedTextSink",
									"storeSettings": {
										"type": "AzureBlobStorageWriteSetting",
										"copyBehavior": {
											"value": "SELECT TARGET_TBL FROM LOAD_CFG",
											"type": "Expression"
										}
									},
									"formatSettings": {
										"type": "DelimitedTextWriteSetting",
										"quoteAllText": true,
										"fileExtension": ".txt"
									}
								},
								"enableStaging": false,
								"translator": {
									"type": "TabularTranslator",
									"mappings": [
										{
											"sink": {
												"ordinal": 1
											}
										}
									]
								}
							},
							"inputs": [
								{
									"referenceName": "AZURESQLDBTABLES",
									"type": "DatasetReference",
									"parameters": {
										"TABLENAME": {
											"value": "LOAD_CFG",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "BLOBFILES",
									"type": "DatasetReference"
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"TABLE_LIST": {
				"type": "string"
			},
			"TABLENAME": {
				"type": "string"
			},
			"lkp": {
				"type": "String"
			}
		}
	}
}