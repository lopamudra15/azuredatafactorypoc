{
	"name": "pipeline2",
	"properties": {
		"activities": [
			{
				"name": "Execute Pipeline1",
				"type": "ExecutePipeline",
				"typeProperties": {
					"waitOnCompletion": true
				}
			},
			{
				"name": "IterateSQLTables",
				"type": "ForEach",
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.TABLE_LIST",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Copy Data2",
							"type": "Copy",
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"typeProperties": {
								"source": {
									"type": "AzureSqlSource",
									"sqlReaderQuery": ""
								},
								"enableStaging": false
							},
							"inputs": [
								{
									"referenceName": "AZURESQLDBTABLES",
									"type": "DatasetReference"
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"TABLE_LIST": {
				"type": "array"
			}
		}
	}
}